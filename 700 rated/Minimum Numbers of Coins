Problem Link:https://www.codechef.com/problems/MINCOINS?tab=statement



#pragma GCC optimize("O2,no-stack-protector,unroll-loops,fast-math")
#include <iostream>
#include <bits/stdc++.h>
#define ll long long
#define PB push_back
#define F first
#define S second
#define uset unordered_set
#define umap unordered_map
#define MP make_pair;
#define vt vector
#define all(n) begin(n), end(n)
#define sz(n) (int)n.size()
#define REP(i, n) for (int i = 0; i < n; i++)
#define pii pair<int, int>;
#define pll pair<ll, ll>;

using namespace std;

typedef vector<ll> vl;
typedef pair<ll,ll> pl;

void solve()
{
    ll x;
    cin>>x;
    if(x%5!=0 ){
        cout<<-1<<'\n';
    }else{
    ll tens=x/10;
    ll remaining_fives=x%10;
    ll fives=remaining_fives/5;
    cout<<tens+fives<<'\n';
    }
}
int main()
{
	ios::sync_with_stdio(0);
	cin.tie(0), cout.tie(0);
   	ll t;
    	cin >> t;
    	while (t--)
    	{
        	solve();
    	}
    	return 0;
}
